{
  "name": "mustafaercin-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --hostname localhost --port 3001",
    "build": "set NEXT_DISABLE_ESLINT_PLUGIN=true && next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && rmdir /s /q docs && ren out docs",
    "deploy": "npm run export && echo. > docs/.nojekyll && git add -f docs && git commit -m \"Deploy to GitHub Pages\" && git push"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-resizable": "^3.0.8",
    "bootstrap": "^5.3.6",
    "framer-motion": "^12.12.2",
    "next": "15.1.8",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-resizable": "^3.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
